{
  "name": "tv-minder-front",
  "version": "0.1.0",
  "author": "Tim Rybicki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trybick/tv-minder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "CI= react-scripts build",
    "lint": "eslint --fix --ext .tsx,.js,.ts .",
    "lint:check": "eslint --max-warnings=0 --ext .tsx,.js,.ts .",
    "start": "cross-env PORT=4000 react-scripts start",
    "start:local": "cross-env PORT=4000 REACT_APP_API=local react-scripts start",
    "validate": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,js,ts}": "eslint --cache --fix"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@fullcalendar/core": "^5.8.0",
    "@fullcalendar/daygrid": "^5.8.0",
    "@fullcalendar/interaction": "^5.8.0",
    "@fullcalendar/react": "^5.8.0",
    "axios": "^0.21.1",
    "emotion-theming": "^10.0.27",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^5.5.3",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-truncate": "^2.4.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-truncate": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "typescript": "^3.7.2"
  },
  "resolutions": {
    "body-scroll-lock": "3.0.3"
  }
}
