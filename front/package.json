{
  "name": "tv-minder-front",
  "version": "0.1.0",
  "author": "Tim Rybicki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trybick/tv-minder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "react-scripts build",
    "lint": "eslint --fix --ext .tsx,.js,.ts .",
    "lint:check": "eslint --max-warnings=0 --ext .tsx,.js,.ts .",
    "start": "cross-env PORT=4000 react-scripts start",
    "start:local": "cross-env PORT=4000 REACT_APP_API=local react-scripts start",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "validate": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,js,ts}": "eslint --cache --fix"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.0",
    "@chakra-ui/react": "^1.0.0",
    "@emotion/react": "11.0.0",
    "@emotion/styled": "^11.0.0",
    "@fullcalendar/core": "^5.1.0",
    "@fullcalendar/daygrid": "^5.1.0",
    "@fullcalendar/interaction": "^5.1.0",
    "@fullcalendar/react": "^5.1.0",
    "@types/react-truncate": "^2.3.3",
    "axios": "^0.19.2",
    "focus-visible": "^5.2.0",
    "framer-motion": "^2.9.4",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-google-login": "^5.1.22",
    "react-hook-form": "^5.5.3",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-truncate": "^2.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "24.0.0",
    "typescript": "^3.8.0"
  }
}
