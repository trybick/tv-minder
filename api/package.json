{
  "name": "tv-minder-api",
  "version": "0.1.0",
  "main": "./dist/server.js",
  "author": "Tim Rybicki",
  "license": "MIT",
  "scripts": {
    "start": "tsc-watch --onSuccess \"nodemon ./dist/server.js\" --onFailure \"Compilation failed\"",
    "serve": "docker-compose up",
    "serve-build": "docker-compose build",
    "serve-build-clean": "docker-compose build --no-cache",
    "serve-stop": "docker-compose down"
  },
  "_moduleAliases": {
    "config": "dist/config",
    "entities": "dist/entities",
    "controllers": "dist/entities/controllers",
    "models": "dist/entities/models",
    "routes": "dist/entities/routes",
    "middleware": "dist/middleware",
    "utils": "dist/utils"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.2",
    "mongoose": "5.7.1",
    "nodemailer": "^6.4.11",
    "ts-mongoose": "^0.0.21"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "5.7.1",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.3",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
